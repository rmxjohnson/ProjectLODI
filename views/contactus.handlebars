<body>

    <div class="container">

        {{> navbar }}
        <form>
            <div class="form-group">
                <label for="nameinput">
                    <strong class="orangeL">Name</strong>
                </label>
                <input type="text" class="form-control" id="name-input" placeholder="Your Name">
            </div>
            <div class="form-group">
                <label for="exampleFormControlInput1">
                    <strong class="greyL">Email Address</strong>
                </label>
                <input type="email" class="form-control" id="email-input" placeholder="name@example.com" title="Please provide only a Best Startup Ever corporate email address">
            </div>
            <div class="form-group">
                <label for="exampleFormControlTextarea1">
                    <strong class="orangeL">Comments:</strong>
                </label>
                <textarea class="form-control" id="text-input" rows="3" placeholder="Leave Us A Comment. "></textarea>
            </div>
            <button type="submit" class="btn btn-primary" id="submit">Submit</button>
        </form>
        <br>
        <br>
        <div id="comments"></div>
    </div>



    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo"
        crossorigin="anonymous"></script>
